

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User  {
  id Int @id @default(autoincrement())
  usernmae String @unique
  email String @unique
  password String @unique
  movieCW Int
  tvCW Int
  movieWL Int
  tvWL Int
}